{
  "version": "0.0.2",
  "keywords": ["yt-transcript", "youtube", "transcript", "strapi", "plugin"],
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulBratslavsky/yt-transcript-strapi-plugin"
  },
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "./strapi-server.js"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@langchain/core": "^0.3.18",
    "@langchain/openai": "^0.3.13",
    "@strapi/design-system": "^2.0.0-rc.12",
    "@strapi/icons": "^2.0.0-rc.12",
    "langchain": "^0.3.5",
    "react-intl": "^6.8.7",
    "youtubei.js": "^11.0.1"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.0.0-rc.30",
    "@strapi/typescript-utils": "^5.0.0-rc.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "styled-components": "^6.1.13",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.0.0-rc.30",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "styled-components": "^6.1.13"
  },
  "strapi": {
    "kind": "plugin",
    "name": "yt-transcript",
    "displayName": "YT Transcript",
    "description": "Get YT Video Transcript"
  },
  "name": "yt-transcript-strapi-plugin",
  "description": "Get YT Video Transcript",
  "license": "MIT",
  "author": "Paul Brats <codingafterthirty@gmail.com>",
  "publishConfig": {
    "access": "public"
  }
}
