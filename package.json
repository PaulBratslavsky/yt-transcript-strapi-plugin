{
  "version": "0.1.4",
  "keywords": [
    "yt-transcript-strapi-plugin",
    "youtube",
    "transcript",
    "strapi",
    "plugin",
    "mcp",
    "model-context-protocol",
    "chatgpt",
    "claude",
    "ai"
  ],
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulBratslavsky/yt-transcript-strapi-plugin"
  },
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "./strapi-server.js"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "test:mcp": "npx ts-node --project scripts/tsconfig.json scripts/test-mcp.ts",
    "test:mcp:local": "npx ts-node --project scripts/tsconfig.json scripts/test-mcp.ts http://localhost:1337",
    "test:mcp:prod": "npx ts-node --project scripts/tsconfig.json scripts/test-mcp.ts https://deserving-harmony-9f5ca04daf.strapiapp.com",
    "dev:public": "./scripts/dev-public.sh",
    "inspect": "npx @modelcontextprotocol/inspector http://localhost:1337/api/yt-transcript-strapi-plugin/mcp",
    "inspect:ngrok": "npx @modelcontextprotocol/inspector https://cinnamyl-lonny-decompressive.ngrok-free.dev/api/yt-transcript-strapi-plugin/mcp"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@strapi/design-system": "^2.0.0-rc.12",
    "@strapi/icons": "^2.0.0-rc.12",
    "react-intl": "^6.8.7",
    "undici": "^6.21.0",
    "youtubei.js": "^16.0.1",
    "zod": "^3.23.0"
  },
  "bundledDependencies": [
    "@modelcontextprotocol/sdk",
    "zod"
  ],
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.33.0",
    "@strapi/typescript-utils": "^5.33.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "styled-components": "^6.1.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.33.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "styled-components": "^6.1.13"
  },
  "engines": {
    "node": ">=18.0.0 <=24.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "kind": "plugin",
    "name": "yt-transcript-strapi-plugin",
    "displayName": "YT Transcript",
    "description": "Get YT Video Transcript"
  },
  "name": "yt-transcript-strapi-plugin",
  "description": "Get YT Video Transcript",
  "license": "MIT",
  "author": "Paul Brats <codingafterthirty@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "bundleDependencies": [
    "@modelcontextprotocol/sdk",
    "zod"
  ]
}
